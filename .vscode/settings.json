{
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.formatOnSave": false,
  "eslint.enable": true,
  "eslint.options": {
    //指定vscode的eslint所处理的文件的后缀
    "extensions": [".js", ".jsx", ".ts", ".tsx", "cjs", "mjs"]
  },
  "eslint.validate": ["javascript", "typescript", "typescriptreact"]
}